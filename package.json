{
  "name": "bff",
  "version": "1.0.0",
  "description": "api gateway for the frontend",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "nodemon src/index.js --watch src --exec babel-node",
    "lint": "eslint src migrations seeds",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest",
    "test:setup": "rm test.sqlite3 || true; knex migrate:latest --env test && knex seed:run --env test"
  },
